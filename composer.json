{
    "name": "passionweb/form-email-contentblocks",
    "type": "typo3-cms-extension",
    "description": "Allows adding a content element at the beginning (introductory text) and/or at the end (email signature) of the email templates. Also, the background color and logo of the fluid email template can be edited.",
    "version": "2.1.0",
    "homepage": "https://www.passionweb.de",
    "authors": [
        {
            "name": "Manuel Schnabel",
            "role": "Developer"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^12.4",
        "typo3/cms-form": "^12.4"
    },
    "autoload": {
        "psr-4": {
            "Passionweb\\FormEmailContentblocks\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/form-email-contentblocks": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "form_email_contentblocks"
        }
    }
}
